{
  "name": "oma-opintopolku-loki",
  "version": "0.0.1",
  "description": "Audit log view for Opintopolku audit logs",
  "license": "EUPL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opetushallitus/oma-opintopolku-loki.git"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "build:dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build:qa": "./node_modules/.bin/webpack --config webpack.qa.js",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.js",
    "build:local": "./node_modules/.bin/webpack --config webpack.local.js",
    "start:local": "npm run mock-api & ./node_modules/.bin/webpack serve --config webpack.local.js",
    "start:local:nologin": "webpack serve --config webpack.local.js",
    "mock-api": "./node_modules/.bin/json-server ./mock/api.json",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src test",
    "test": "npm run build:prod && JEST_ENV=jest-environment-puppeteer ./node_modules/.bin/jest --testPathPattern=page.spec.js",
    "unit": "JEST_ENV=jsdom ./node_modules/.bin/jest --testPathPattern=test.js --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "node update_nvmrc.js && npm run snyk-protect",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "ua-parser-js": "^0.7.24"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/webpack": "^6.2.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "1.13.1",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.30.0",
    "eslint-config-standard": "16.0.3",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "jest": "^27.5.0",
    "jest-cli": "^27.5.0",
    "jest-puppeteer": "^6.1.0",
    "json-server": "^0.17.0",
    "minimist": "1.2.5",
    "puppeteer": "^13.1.3",
    "react-test-renderer": "^16.14.0",
    "snyk": "^1.848.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "axios": "0.21.4",
    "core-js": "^3.21.0",
    "date-fns": "^2.28.0",
    "dotenv": "10.0.0",
    "js-cookie": "2.2.1",
    "merge": "2.1.1",
    "prop-types": "^15.8.1",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "save-dev": "0.0.1-security",
    "styled-components": "^5.3.3"
  },
  "snyk": true
}
