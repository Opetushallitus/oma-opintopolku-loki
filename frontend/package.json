{
  "name": "oma-opintopolku-loki",
  "version": "0.0.1",
  "description": "Audit log view for Opintopolku audit logs",
  "license": "EUPL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opetushallitus/oma-opintopolku-loki.git"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build:dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build:qa": "./node_modules/.bin/webpack --config webpack.qa.js",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.js",
    "build:local": "./node_modules/.bin/webpack --config webpack.local.js",
    "start:local": "npm run mock-api & ./node_modules/.bin/webpack serve --config webpack.local.js",
    "start:local:nologin": "webpack serve --config webpack.local.js",
    "mock-api": "node ./mock/server.js",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src test",
    "test": "npm run build:prod && JEST_ENV=jest-environment-puppeteer ./node_modules/.bin/jest --testPathPattern=page.spec.js",
    "unit": "JEST_ENV=jsdom ./node_modules/.bin/jest --testPathPattern=test.js --coverage",
    "unit:update-snapshots": "JEST_ENV=jsdom ./node_modules/.bin/jest --testPathPattern=test.js --coverage --updateSnapshot",
    "prepare": "node update_nvmrc.js"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@svgr/webpack": "6.2.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "9.1.3",
    "babel-plugin-styled-components": "2.1.4",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-webpack-plugin": "4.0.1",
    "html-webpack-plugin": "5.6.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-puppeteer": "^11.0.0",
    "json-server": "0.17.4",
    "minimist": "1.2.8",
    "puppeteer": "24.6.0",
    "react-test-renderer": "17.0.2",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.1",
    "webpack-merge": "5.10.0"
  },
  "dependencies": {
    "@babel/runtime": "7.27.0",
    "axios": "1.8.4",
    "core-js": "3.41.0",
    "date-fns": "2.30.0",
    "dotenv": "16.4.7",
    "js-cookie": "3.0.5",
    "merge": "2.1.1",
    "prop-types": "15.8.1",
    "ramda": "0.30.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "save-dev": "0.0.1-security",
    "styled-components": "5.3.11"
  },
  "snyk": true
}
