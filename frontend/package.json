{
  "name": "oma-opintopolku-loki",
  "version": "0.0.1",
  "description": "Audit log view for Opintopolku audit logs",
  "license": "EUPL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opetushallitus/oma-opintopolku-loki.git"
  },
  "engines": {
    "node": "14"
  },
  "scripts": {
    "build:dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build:qa": "./node_modules/.bin/webpack --config webpack.qa.js",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.js",
    "build:local": "./node_modules/.bin/webpack --config webpack.local.js",
    "start:local": "npm run mock-api & ./node_modules/.bin/webpack-dev-server --config webpack.local.js",
    "mock-api": "./node_modules/.bin/json-server ./mock/api.json",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src test",
    "test": "npm run build:prod && JEST_ENV=jest-environment-puppeteer ./node_modules/.bin/jest --testPathPattern=page.spec.js",
    "unit": "JEST_ENV=jsdom ./node_modules/.bin/jest --testPathPattern=test.js --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "node update_nvmrc.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.7.1",
    "clean-webpack-plugin": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.7.1",
    "jest-puppeteer": "^3.5.2",
    "json-server": "^0.15.1",
    "minimist": ">=1.2.2",
    "puppeteer": "1.20.0",
    "react-test-renderer": "^16.6.3",
    "snyk": "^1.299",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "axios": "0.21.1",
    "core-js": "3.9.1",
    "date-fns": "2.19.0",
    "dotenv": "8.2.0",
    "js-cookie": "2.2.1",
    "merge": "2.1.1",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "recompose": "0.30.0",
    "save-dev": "0.0.1-security",
    "styled-components": "4.0.3"
  },
  "snyk": true
}
