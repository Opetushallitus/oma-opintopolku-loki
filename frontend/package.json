{
  "name": "oma-opintopolku-loki",
  "version": "0.0.1",
  "description": "Audit log view for Opintopolku audit logs",
  "license": "EUPL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opetushallitus/oma-opintopolku-loki.git"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "build:dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build:qa": "./node_modules/.bin/webpack --config webpack.qa.js",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.js",
    "build:local": "./node_modules/.bin/webpack --config webpack.local.js",
    "start:local": "npm run mock-api & ./node_modules/.bin/webpack serve --config webpack.local.js",
    "mock-api": "./node_modules/.bin/json-server ./mock/api.json",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src test",
    "test": "npm run build:prod && JEST_ENV=jest-environment-puppeteer ./node_modules/.bin/jest --testPathPattern=page.spec.js",
    "unit": "JEST_ENV=jsdom ./node_modules/.bin/jest --testPathPattern=test.js --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "node update_nvmrc.js && npm run snyk-protect",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "ua-parser-js": "^0.7.24"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@svgr/webpack": "5.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.13.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.30.0",
    "eslint-config-standard": "16.0.3",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "5.0.0",
    "html-webpack-plugin": "5.3.2",
    "http-server": "0.12.3",
    "jest": "27.0.6",
    "jest-cli": "27.0.6",
    "jest-puppeteer": "5.0.4",
    "json-server": "0.16.3",
    "minimist": "1.2.5",
    "puppeteer": "10.1.0",
    "react-test-renderer": "^16.14.0",
    "snyk": "1.655.0",
    "webpack": "5.42.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0",
    "yargs": "17.0.1"
  },
  "dependencies": {
    "@babel/runtime": "7.14.6",
    "axios": "0.21.1",
    "core-js": "3.15.2",
    "date-fns": "2.22.1",
    "dotenv": "10.0.0",
    "js-cookie": "2.2.1",
    "merge": "2.1.1",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "recompose": "0.30.0",
    "save-dev": "0.0.1-security",
    "styled-components": "5.3.0"
  },
  "snyk": true
}
